	<owl:Class rdf:about="https://spec.industrialontologies.org/ontology/qualities/Qualities-MeasurementPerformance/CalibrationRange">
		<rdfs:subClassOf rdf:resource="https://spec.industrialontologies.org/ontology/qualities/Qualities-Physical/DynamicRange"/>
		<rdfs:label rdf:datatype="&rdf;langString" xml:lang="de">Kalibrierbereich</rdfs:label>
		<rdfs:label rdf:datatype="&rdf;langString" xml:lang="en">calibration range</rdfs:label>
		<rdfs:isDefinedBy rdf:resource="https://w3id.org/pmd/co"/>
		<iof-av:naturalLanguageDefinition rdf:datatype="&rdf;langString" xml:lang="en">Calibration range is the region between limits within which a quantity is measured, received, or transmitted, typically by a process node like a machine, expressed through lower and upper range values</iof-av:naturalLanguageDefinition>
	</owl:Class>
	
	<owl:Class rdf:about="https://spec.industrialontologies.org/ontology/qualities/Qualities-MeasurementPerformance/DataAcquisitionRate">
		<rdfs:subClassOf rdf:resource="&iof-core;ProcessCharacteristic"/>
		<rdfs:label rdf:datatype="&rdf;langString" xml:lang="de">Datenerfassungsrate</rdfs:label>
		<rdfs:label rdf:datatype="&rdf;langString" xml:lang="en">data acquisition rate</rdfs:label>
		<rdfs:isDefinedBy rdf:resource="https://w3id.org/pmd/co"/>
		<iof-av:naturalLanguageDefinition rdf:datatype="&rdf;langString" xml:lang="en">Data acquisition rate is the number of measurements or queries per time period for a considered measurement of a parameter, representing sampling or query rate during data acquisition</iof-av:naturalLanguageDefinition>
	</owl:Class>
	
	<owl:Class rdf:about="https://spec.industrialontologies.org/ontology/qualities/Qualities-MeasurementPerformance/MeasurementRelationalQuality">
		<rdfs:subClassOf rdf:resource="&obo;BFO_0000145"/>
		<rdfs:label rdf:datatype="&rdf;langString" xml:lang="en">measurement relational quality</rdfs:label>
		<rdfs:label rdf:datatype="&rdf;langString" xml:lang="de">messungsrelationale Qualität</rdfs:label>
		<iof-av:naturalLanguageDefinition rdf:datatype="&rdf;langString" xml:lang="en">measurement relational quality is a relational quality that pertains to the measurement attributes related to other entities, requiring another entity for its existence</iof-av:naturalLanguageDefinition>
	</owl:Class>
	
	<owl:Class rdf:about="https://spec.industrialontologies.org/ontology/qualities/Qualities-MeasurementPerformance/PerformanceQuality">
		<rdfs:subClassOf rdf:resource="&obo;BFO_0000019"/>
		<rdfs:label rdf:datatype="&rdf;langString" xml:lang="de">Leistungsqualität</rdfs:label>
		<rdfs:label rdf:datatype="&rdf;langString" xml:lang="en">performance quality</rdfs:label>
		<iof-av:naturalLanguageDefinition rdf:datatype="&rdf;langString" xml:lang="en">Performance quality is an attribute related to the effectiveness or efficiency of an entity’s performance, typically evaluated in relation to a standard or requirement</iof-av:naturalLanguageDefinition>
	</owl:Class>
	
	<owl:Class rdf:about="https://spec.industrialontologies.org/ontology/qualities/Qualities-MeasurementPerformance/Precision">
		<rdfs:subClassOf rdf:resource="https://spec.industrialontologies.org/ontology/qualities/Qualities-MeasurementPerformance/MeasurementRelationalQuality"/>
		<rdfs:label rdf:datatype="&rdf;langString" xml:lang="de">Genauigkeit</rdfs:label>
		<rdfs:label rdf:datatype="&rdf;langString" xml:lang="en">precision</rdfs:label>
		<rdfs:isDefinedBy rdf:resource="https://w3id.org/pmd/co"/>
		<iof-av:adaptedFrom rdf:datatype="&rdf;langString" xml:lang="en">“Precision.” Merriam-Webster.com Dictionary, Merriam-Webster, https://www.merriam-webster.com/dictionary/precision. Accessed 13 Jan. 2023.</iof-av:adaptedFrom>
		<iof-av:naturalLanguageDefinition rdf:datatype="&rdf;langString" xml:lang="en">Precision is the degree of refinement with which an operation is performed or a measurement stated</iof-av:naturalLanguageDefinition>
	</owl:Class>
	
	<owl:Class rdf:about="https://spec.industrialontologies.org/ontology/qualities/Qualities-MeasurementPerformance/WorkingRange">
		<rdfs:subClassOf rdf:resource="https://spec.industrialontologies.org/ontology/qualities/Qualities-Physical/DynamicRange"/>
		<rdfs:label rdf:datatype="&rdf;langString" xml:lang="de">Arbeitsbereich</rdfs:label>
		<rdfs:label rdf:datatype="&rdf;langString" xml:lang="en">working range</rdfs:label>
		<rdfs:isDefinedBy rdf:resource="https://w3id.org/pmd/co"/>
		<iof-av:naturalLanguageDefinition rdf:datatype="&rdf;langString" xml:lang="en">Working range is the region between limits within which a processing node, like a machine, is usable for its intended purpose, expressed through lower and upper range values of interest</iof-av:naturalLanguageDefinition>
	</owl:Class>
	
	<owl:Class rdf:about="https://spec.industrialontologies.org/ontology/qualities/Qualities-Physical/DynamicRange">
		<rdfs:subClassOf rdf:resource="https://spec.industrialontologies.org/ontology/qualities/Qualities-MeasurementPerformance/MeasurementRelationalQuality"/>
		<rdfs:label rdf:datatype="&rdf;langString" xml:lang="de">Dynamikbereich</rdfs:label>
		<rdfs:label rdf:datatype="&rdf;langString" xml:lang="en">dynamic range</rdfs:label>
		<iof-av:adaptedFrom rdf:datatype="&rdf;langString" xml:lang="en">https://en.wikipedia.org/wiki/Dynamic_range</iof-av:adaptedFrom>
		<iof-av:naturalLanguageDefinition rdf:datatype="&rdf;langString" xml:lang="en">Dynamic range is the ratio between the largest and smallest values a quantity can assume, often used in signal contexts like sound and light, measured as a ratio or logarithmic value in base-10 or base-2</iof-av:naturalLanguageDefinition>
	</owl:Class>

</rdf:RDF>